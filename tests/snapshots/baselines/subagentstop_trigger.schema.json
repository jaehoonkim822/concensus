{
  "description": "SubagentStop hook triggers consensus review on Explore/Plan agent output",
  "required_keys": ["systemMessage"],
  "pattern_checks": {
    "has_consensus_header": "\\[CONSENSUS REVIEW - SUBAGENT\\]",
    "has_result_line": "Result:\\s+(FULL_CONSENSUS|MAJORITY_AGREE|NO_CONSENSUS|SKIPPED)",
    "has_round_indicator": "round \\d+",
    "has_model_verdict": "(Approve:|Concerns:)",
    "has_process_log": "Queried:"
  }
}
